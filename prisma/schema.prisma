model User {
  id      String     @id @unique 
  name    String
  profilepic String
  posts   Post[]
  Phonenumber Int 
  Collaborations String[]
}


model Post {
  id         String       @id @unique 
  title      String
  content    String
  authorId   String
  author     User       @relation(fields: [authorId], references: [id])
  categories String
  Collaborators String[]
}

